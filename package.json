{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -rf build",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "build": "npm run clean && npm run tsc",
    "build:w": "npm run clean && npm run tsc:w",
    "start": "npm run ts -- src/index.ts",
    "watch": "npm run nd -- src/index.ts",
    "ts": "ts-node",
    "nd": "nodemon --exec ts-node",
    "bs": "npm run build && node"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.77",
    "@types/node": "^8.0.33",
    "nodemon": "^1.14.12",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
